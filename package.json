    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "auto": "^10.37.6",
    "babel-loader": "^8.2.5",
    "husky": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "size-limit": "^8.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
